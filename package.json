{
  "name": "jsonblite",
  "description": "JSONBLite is a single file, key-value binary JSON database, written as TypeScript class for Node.js. A simple, persistent JSON/CBOR storage.",
  "version": "0.0.1",
  "main": "dist/src/jsonblite.js",
  "types": "dist/src/jsonblite.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "node --experimental-strip-types test/jsonblite.test.ts && node --experimental-strip-types test/acid.test.ts",
    "benchmark": "node --experimental-strip-types test/benchmark.test.ts"
  },
  "dependencies": {
    "cbor-x": "^1.6.0",
    "fs-ext": "^2.1.1"
  },
  "devDependencies": {
    "@types/fs-ext": "^2.0.3",
    "@types/node": "^25.3.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "README.md",
    "src"
  ],
  "author": "Matias Kaskimies",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mkaski/jsonblite.git"
  },
  "keywords": [
    "json",
    "cbor",
    "binary",
    "database",
    "key-value",
    "single-file"
  ]
}
